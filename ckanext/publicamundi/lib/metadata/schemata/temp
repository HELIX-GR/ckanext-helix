class IInspireDataThemes(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = True)

    terms = zope.schema.List(
        title = u"Terms",
        required = True,
        min_length = 1,
        value_type = zope.schema.Choice(Helper.flatten_dict_vals(Helper.get_keyword_dict('inspire_data_themes').get('terms'))))

class IInspireFeatureConcept(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = False)
    
    terms = zope.schema.List(
            title = u"Terms",
            required = False,
            value_type = zope.schema.Choice(Helper.flatten_dict_vals(Helper.get_keyword_dict('inspire_feature_concept_dictionary').get('terms'))))
 
class IInspireGlossary(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = False)
    
    terms = zope.schema.List(
            title = u"Terms",
            required = False,
            value_type = zope.schema.Choice(Helper.flatten_dict_vals(Helper.get_keyword_dict('inspire_glossary').get('terms'))))

class IGemetConcepts(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = False)
    
    terms = zope.schema.List(
            title = u"Terms",
            required = False,
            value_type = zope.schema.Choice(Helper.flatten_dict_vals(Helper.get_keyword_dict('gemet_concepts').get('terms'))))

class IGemetGroups(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = False)
    
    terms = zope.schema.List(
            title = u"Terms",
            required = False,
            value_type = zope.schema.Choice(Helper.flatten_dict_vals(Helper.get_keyword_dict('gemet_groups').get('terms'))))

class IGemetSupergroups(IObject):
    thesaurus = zope.schema.Object(IThesaurus,
            required = False)

